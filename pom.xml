		<hibernateversion>3.3.2.GA</hibernateversion>
		<junitversion>4.9</junitversion>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<profiles.dir>src/profiles</profiles.dir>
	</properties>
	<profiles>
		<profile>
			<id>dev</id>
			<build>
				<finalName>jmapper-web</finalName>
				<plugins>

					<plugin>
